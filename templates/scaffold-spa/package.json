    "stylelint": "^14.11.0",
    "stylelint-config-standard": "^28.0.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@johnsonandjohnson/mettle-components": "^1.10.3",
